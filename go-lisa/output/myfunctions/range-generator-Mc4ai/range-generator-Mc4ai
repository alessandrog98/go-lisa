package main

import (
    "fmt"
)

//BEGIN
func generateArray(start int, end int) ([]int, int) {
    var arr []int
    var sign int
    if start < 0 && end < 0 {
        sign = -1
    } else if start > 0 && end > 0 {
        sign = 1
    } else {
        sign = 0
    }
    for i := start; i <= end; i++ {
        arr = append(arr, i)
    }
    return arr, sign
}

func main() {
    arr1, sign1 := generateArray(2, 5)
}